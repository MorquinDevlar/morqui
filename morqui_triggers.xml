      cechoLink(" &lt;white&gt;#"..v.." &lt;green&gt;", [[send("moveto ]]..v..[[")]], "Move to quest "..v, true)
      cecho("&lt;white&gt;(&lt;LightSkyBlue&gt;"..countQuestItems(x1).."&lt;white&gt;)")   
    end
  end

